{
    "manifest_version": 3,
    "name": "DS DOM Counter",
    "version": "1.0",
    "description": "Compteur de DOM pour le Design System",
    "permissions": [
        "activeTab",
        "storage",
        "scripting"
    ],
    "host_permissions": [
        "*://design-system.docs.pole-emploi.intra/*"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}